import java.util.Scanner;
class Loop{
	public static void main(String[]args){
	int N;
	Scanner sc=new Scanner(System.in);
	System.out.println("enter the number");
	N=sc.nextInt();
            for(int i=1;i<10;i++){
                System.out.println(N + " x " + i + " = " + N*i);
                }

	}
}
